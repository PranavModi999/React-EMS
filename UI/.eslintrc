{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "import/extensions": ["error", "always", { "ignorePackages": true }],
    "linebreak-style": "off",
    "react/prop-types": "off"
  }
}
